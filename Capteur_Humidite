
//Pour utiliser ce code, veuillez aux préalable télécharger la bibliothèque: dht.h

//Ce code permet de visualiser les valeurs que capte le capteur d'humidite qui seront afficher sur le moniteur serie du logiciel arduino

#include <dht.h>

dht DHT;

#define DHT11_PIN 7

void setup(){
  Serial.begin(9600);
}

void loop(){
  int chk = DHT.read11(DHT11_PIN);
  Serial.print("Temperature = ");
  Serial.println(DHT.temperature);
  Serial.print("Humidity = ");
  Serial.println(DHT.humidity);
  delay(1000);
}
